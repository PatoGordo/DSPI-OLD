{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "baseUrl": "."
  },
  "include": [
    "*"
  ],
  "exclude": [
    "dist"
  ]
}